{% extends "::base.html.twig" %}

{% block title %}EDD Users{% endblock %}

{% block body %}
    <h3>EDD Users</h3>

    {% if users|length >0 %}

        <ul>
            {% for user in users %}
                <li>{{ user.username }} - <a href="{{ path('readUser',{id:user.id}) }}">profile</a></li>
            {% endfor %}
        </ul>

    {% else %}

        <p>No users in Database</p>

    {% endif %}

    <a href="{{ path('index') }}">Back</a>


{% endblock %}
